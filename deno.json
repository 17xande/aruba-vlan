{
  "tasks": {
    "start": "deno run --allow-net --allow-env --allow-read --unsafely-ignore-certificate-errors main.ts",
    "dev": "deno run --watch main.ts",
    "compile": "deno compile --allow-net --allow-env --allow-read --unsafely-ignore-certificate-errors --env-file=.env main.ts",
    "compile-win": "deno compile --allow-net --allow-env --allow-read --unsafely-ignore-certificate-errors --target x86_64-pc-windows-msvc --icon ./icon.ico --env-file=.env --no-terminal --output ./TV-switch.exe main.ts"

  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@^1.0.21",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/http": "jsr:@std/http@^1.0.20"
  }
}
